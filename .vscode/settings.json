{
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "files.trimTrailingWhitespace": true,
    "editor.formatOnSave": true,
    "editor.trimAutoWhitespace": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/*.freezed.dart": false,
    "**/*.g.dart": false,
    "**/*.gr.dart": false,
    "**/.gitkeep": false
  }
}
